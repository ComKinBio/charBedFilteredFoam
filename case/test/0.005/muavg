FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.005";
    object      muavg;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 2.042700332e-05;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 1.835366622e-05;
    }
    outlet
    {
        type            calculated;
        value           uniform 2.042700332e-05;
    }
    walls
    {
        type            calculated;
        value           uniform 2.042700332e-05;
    }
}


