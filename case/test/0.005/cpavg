FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0.005";
    object      cpavg;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 -1 0 0 0];

internalField   uniform 1014.093235;

boundaryField
{
    inlet
    {
        type            calculated;
        value           uniform 1011.338357;
    }
    outlet
    {
        type            calculated;
        value           uniform 1014.093235;
    }
    walls
    {
        type            calculated;
        value           uniform 1014.093235;
    }
}


